<Defs>


<!--flooring-->
<RecipeDef>
    <defName>BarkCutting</defName>
    <label>cut the bark from a wood log</label>
    <description>Cut the bark from a wood log used for mulch flooring.</description>
    <jobString>Cutting Bark.</jobString>
    <workAmount>600</workAmount>
    <workSpeedStat>SculptingSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Sculpt</effectWorking>
      <soundWorking>Recipe_Sculpt</soundWorking>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>WoodLog</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
    </ingredients>
    <products>
      <WGBark>20</WGBark>
    </products>
	<researchPrerequisite>WGFloorRes</researchPrerequisite>
    <recipeUsers>
        <li>CraftingSpot</li>
    </recipeUsers>
  </RecipeDef>
  
  <RecipeDef Name="WGCookMealBase" Abstract="True">
		<fixedIngredientFilter>
			<categories>
				<li>PlantFoodRaw</li>
			</categories>
			<specialFiltersToDisallow>
				<li>AllowPlantFood</li>
			</specialFiltersToDisallow>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>PlantFoodRaw</li>
			</categories>
		</defaultIngredientFilter>
		<workSkill>Cooking</workSkill>
	</RecipeDef>
  
  <RecipeDef ParentName="WGCookMealBase">
    <defName>WGCompostStarterMaking</defName>
    <label>make compost starter from veggetables</label>
    <description>Make compost starter from veggetables used for better flooring.</description>
    <jobString>Making compost starter</jobString>
    <workAmount>550</workAmount>
    <workSpeedStat>ButcheryFleshSpeed</workSpeedStat>
    <effectWorking>ButcherFlesh</effectWorking>
    <soundWorking>Recipe_ButcherCorpseFlesh</soundWorking>
	<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <ingredients>
      <li>
				<filter>
					<categories>
						<li>PlantFoodRaw</li>
					</categories>
				</filter>
				<count>0.25</count>
			</li>
    </ingredients>
    <products>
      <WGCompostStarter>10</WGCompostStarter>
    </products>
	<researchPrerequisite>WGFloorRes</researchPrerequisite>
    <recipeUsers>
        <li>TableButcher</li>
    </recipeUsers>
  </RecipeDef>
  
  
